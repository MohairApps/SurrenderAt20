    xmlns:views="clr-namespace:MvvmCross.Forms.Views;assembly=MvvmCross.Forms"
    xmlns:viewModels="clr-namespace:Surrender_20.Forms.ViewModels"
    xmlns:local="clr-namespace:Surrender_20.Forms.Views"
    x:Class="Surrender_20.Forms.Views.RootPage"
    Title="{Binding Title}">

    <TabbedPage.Children>
        <local:NewsfeedItemView/>
        <local:NewsfeedItemView/>
        <local:NewsfeedItemView/>
        <local:NewsfeedItemView/>
    </TabbedPage.Children>

</views:MvxTabbedPage>